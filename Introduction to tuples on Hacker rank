def average(array):
    # your code goes here
    distinct_heights = set(arr)
    average_heights = sum( distinct_heights) / len( distinct_heights)
    
    return( average_heights)
    
if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().split()))
    result = average(arr)
    print(result)
